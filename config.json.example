{
    "model": {
        "name": "DeepSeek/ai-base",
        "max_length": 512,
        "temperature": 0.7,
        "device": "cuda"
    },
    "memory": {
        "short_term_size": 10,
        "long_term_size": 1000
    },
    "learning": {
        "batch_size": 8,
        "learning_rate": 1e-5,
        "max_files": 100
    },
    "api": {
        "deepseek_key": "your_api_key_here",
        "timeout": 10
    },
    "plugins": {
        "enabled": ["ExamplePlugin", "RealTimeSearch"],
        "disabled": ["ExperimentalPlugin"]
    }
}
